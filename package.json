    },
    "dependencies": {
        "bootstrap": "5.2.3"
    },
    "devDependencies": {
        "autoprefixer": "10.4.14",
        "browser-sync": "^3.0.4",
        "chokidar": "3.5.3",
        "concurrently": "6.3.0",
        "postcss": "^8.5.6",
        "prettier": "2.8.6",
        "pug": "^3.0.3",
        "sass": "1.60.0",
        "shelljs": "0.8.5",
        "upath": "2.0.1"
    }
